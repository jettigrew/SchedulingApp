<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DisplayReportsController">
    <children>
        <VBox prefHeight="600.0" prefWidth="950.0">
            <children>
                <Label alignment="CENTER" prefHeight="131.0" prefWidth="950.0" text="Reports" textAlignment="CENTER">
                    <font>
                        <Font name="Verdana" size="45.0" />
                    </font>
                </Label>
                <TabPane fx:id="reportsTabPane" prefHeight="320.0" prefWidth="950.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab fx:id="reportTab1" text="Report 1">
                     <content>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0" style="-fx-border-color: lightgray;">
                                 <children>
                                    <Label text="Appointment Type Count by Month">
                                       <HBox.margin>
                                          <Insets left="5.0" />
                                       </HBox.margin>
                                       <font>
                                          <Font size="20.0" />
                                       </font>
                                    </Label>
                                 </children>
                                 <padding>
                                    <Insets left="5.0" right="5.0" />
                                 </padding>
                              </HBox>
                              <TextArea fx:id="report1TextArea" prefHeight="263.0" prefWidth="900.0" style="-fx-opacity: 1.0;">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </TextArea>
                           </children>
                        </VBox>
                     </content>
                        </Tab>
                        <Tab fx:id="reportTab2" text="Report 2">
                     <content>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <HBox alignment="CENTER_LEFT" minHeight="30.0" prefHeight="25.0" prefWidth="900.0" style="-fx-border-color: lightgray;">
                                 <children>
                                    <Label prefHeight="30.0" prefWidth="292.0" text="Appointments by Contact">
                                       <font>
                                          <Font size="20.0" />
                                       </font>
                                    </Label>
                                    <Label text=" Choose contact:  ">
                                       <font>
                                          <Font size="16.0" />
                                       </font>
                                    </Label>
                                    <ComboBox fx:id="report2ContactCombo" prefHeight="25.0" prefWidth="122.0" promptText="Contact ID" />
                                    <VBox prefHeight="200.0" prefWidth="100.0" />
                                 </children>
                                 <padding>
                                    <Insets left="5.0" right="5.0" />
                                 </padding>
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin>
                              </HBox>
                                      <TableView fx:id="report2Table" prefHeight="550.0" prefWidth="850.0">
                                          <columns>
                                              <TableColumn fx:id="report2Appointment_IDCol" prefWidth="99.20001220703125" text="Appointment_ID" />
                                              <TableColumn fx:id="report2TitleCol" prefWidth="95.20004272460938" text="Title" />
                                              <TableColumn fx:id="report2DescriptionCol" prefWidth="106.79994201660156" text="Description" />
                                              <TableColumn fx:id="report2LocationCol" prefWidth="98.0" text="Location" />
                                              <TableColumn fx:id="report2TypeCol" prefWidth="84.79998779296875" text="Type" />
                                              <TableColumn fx:id="report2ContactCol" prefWidth="93.5999755859375" text="Contact" />
                                              <TableColumn fx:id="report2StartCol" prefWidth="118.40008544921875" text="Start Date/Time" />
                                              <TableColumn fx:id="report2EndCol" prefWidth="119.9998779296875" text="End Date/Time" />
                                              <TableColumn fx:id="report2Customer_IDCol" prefWidth="85.2000732421875" text="Customer_ID" />
                                          </columns>
                                      </TableView>
                           </children>
                        </VBox>
                     </content>
                        </Tab>
                        <Tab fx:id="reportTab3" text="Report 3">
                     <content>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0" style="-fx-border-color: lightgray;">
                                 <children>
                                    <Label text="Customer Count by Country">
                                       <font>
                                          <Font size="20.0" />
                                       </font>
                                    </Label>
                                 </children>
                                 <padding>
                                    <Insets left="5.0" right="5.0" />
                                 </padding>
                              </HBox>
                              <TextArea fx:id="report3TextArea" prefHeight="264.0" prefWidth="900.0" style="-fx-opacity: 1.0;">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </TextArea>
                           </children>
                        </VBox>
                     </content>
                        </Tab>
                    </tabs>
                    <padding>
                        <Insets left="25.0" right="25.0" />
                    </padding>
                </TabPane>
                <ButtonBar prefHeight="50.0" prefWidth="200.0">
                    <buttons>
                        <Button layoutX="785.0" layoutY="10.0" minWidth="62.0" mnemonicParsing="false" onAction="#runReportHandler" prefHeight="35.0" prefWidth="70.0" text="Run">
                            <font>
                                <Font size="14.0" />
                            </font></Button>
                    </buttons>
                    <padding>
                        <Insets left="50.0" right="30.0" top="15.0" />
                    </padding>
                </ButtonBar>
                <Hyperlink onAction="#appointmentsSwitchHandler" text="Appointments">
                    <VBox.margin>
                        <Insets left="25.0" />
                    </VBox.margin>
                    <font>
                        <Font size="18.0" />
                    </font>
                </Hyperlink>
                <Hyperlink layoutX="10.0" layoutY="518.0" onAction="#customersSwitchHandler" text="Customers">
                    <VBox.margin>
                        <Insets left="25.0" />
                    </VBox.margin>
                    <font>
                        <Font size="18.0" />
                    </font>
                </Hyperlink>
            </children>
        </VBox>
    </children>
</AnchorPane>
